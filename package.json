  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Microsoft/PowerBI-visuals-tools/issues"
  },
  "homepage": "https://github.com/Microsoft/PowerBI-visuals-tools#readme",
  "dependencies": {
    "chalk": "2.4.1",
    "commander": "2.16.0",
    "connect": "3.6.6",
